#include <stdio.h>
#define M 4
#define N 5
 
void printCommonElements(int mat[M][N])
{
   
    for (int j = 0; j < N; j++)
    {   
        int x = mat[0][j], count = 0;
        
        for (int i = 1; i < M; i++){
            int flag = 0;
            for(int j = 0; j < N; j++){
                 if(x==mat[i][j]){
                     flag = 1;
                     mat[i][j] = -1;
                     break;
                 }        
            }
            if(flag==1){
                count++;   
            }
        }
        
        if (count==M-1)
            printf("%d ", x);
            
    }
   
}
 
int main()
{
    int mat[M][N] =
    {
        {10, 20, 10, 40, 80},
        {30, 70, 80, 50, 10},
        {80, 70, 70, 30, 10},
        {80, 10, 20, 70, 90},
    };
 
    printCommonElements(mat);
 
    return 0;
}
